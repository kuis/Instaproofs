- var config = require('app').config;
.content: .body
  .contents
  a.logo(href= config.logo_link): img(src= config.logo_url)
  .search-bar
    input.event-search(type="text")&attributes(this.lzA('placeholder', 'Search Events...'))
    input.photo-search(type="text")&attributes(this.lzA('placeholder', 'Search Photos...'))
    .cancel
    .loader.white.small
